{
	"compilerOptions": {
	  "target": "es2020",
	  "module": "es2020",
	  "outDir": "./dist",
	  "rootDir": "./src/server",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": [
	  "src/server/*.ts"
	],
	"exclude": [
	  "node_modules"
	]
  }
  